@model BioForeignKit.Models.Diagnostics

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12">
    <a href="@Url.Action("Index","Diagnostics")" class="btn btn-warning" id="buttonWarning"><i class="fa fa-backward"></i>   Geri Dön</a>
</div>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="alert-icon shadow-reset wrap-alert-b">
                <div class="alert-title">
                    <h2></h2>
                </div>
                <div class="alert alert-info alert-st-two" role="alert">
                    <i class="fa fa-info-circle adminpro-inform admin-check-pro admin-check-pro-none" aria-hidden="true"></i>
                    <p class="message-mg-rt message-alert-none"><h4><strong>Tanı Detay</strong></h4></p>
                </div>

            </div>
            <div class="hpanel">
                <div class="panel-body">
                    <div>

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayName("Tanı:")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.DiagnosticName)
                            </dd>

                            <dt>
                                @Html.DisplayName("Tanı Tanımı:")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Description)
                            </dd>
                        </dl>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            @Html.ActionLink("Düzenle", "Edit", new { id = Model.Id }, new { @class = "btn btn-success",style="color:white" })

                        </div>
                    </div>






                        </div>
                    </div>
                </div></div></div>


            
